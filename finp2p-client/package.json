{
  "name": "@owneraio/finp2p-client",
  "version": "0.27.1",
  "description": "FinP2P API Client",
  "homepage": "https://ownera.io/",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prebuild": "eslint ./src --ext .ts --fix",
    "build": "tsc",
    "postbuild": "cpx 'src/oss/graphql/*.graphql' dist/oss/graphql/",
    "api-generate": "npx openapi-typescript ./apis/application-api.base.yaml -o src/finapi/model-gen.ts",
    "op-api-generate": "npx openapi-typescript ./apis/operational-api.yaml -o src/finapi/op-model-gen.ts && npx ts-node scripts/postprocess-model-gen.ts src/finapi/op-model-gen.ts",
    "graphql-gen": "graphql-codegen --config grahpqlgen.ts",
    "generate-all": "npm run api-generate && npm run op-api-generate && npm run graphql-gen"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "graphql": "^16.8.1",
    "graphql-import-node": "^0.0.5",
    "openapi-fetch": "0.14.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "cpx": "^1.5.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.8.4"
  }
}
